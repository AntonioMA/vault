{{debugger}}
<PageHeader as |p|>
  <p.top>
    <nav class="breadcrumb">
      <ul>
        <li>
          <span class="sep">
            /
          </span>
          {{#link-to
            "vault.cluster.access.method.item.list"
            (pluralize itemType)
          }}
            {{pluralize itemType}}
          {{/link-to}}
        </li>
      </ul>
    </nav>
  </p.top>
  <p.levelLeft>
    <h1 class="title is-3">
      {{model.id}}
    </h1>
  </p.levelLeft>
  {{!-- <p.levelRight>
    {{#link-to
      "vault.cluster.access.method.edit"
      (pluralize itemType)
      model.id
      class="button has-icon-right is-ghost is-compact"
      data-test-entity-edit-link=true
    }}
      Edit{{itemType}}{{i-con glyph="chevron-right" size=11}}
    {{/link-to}}
  </p.levelRight> --}}
</PageHeader>
<div class="box is-shadowless is-marginless is-fullwidth">
  {{info-table-row label="ID" value=id}}
  {{#each-in props as |name value|}}
    {{info-table-row
      alwaysRender=true
      label=(capitalize name)
      value=(stringify value)
    }}
  {{/each-in}}
</div>